<h1>Welcome!</h1>
<form>
  <fieldset>
    <p>
      <label>Login</label>
      <input type="text" name="user" value="<%= obj.user || "" %>" placeholder="User name">
    </p>
    <p>
      <label>Password</label>
      <input type="password" name="pass">
    </p>
    <div>
      <button type="submit" class="btn btn-primary">Login</button>
    </div>
  </fieldset>
</form>